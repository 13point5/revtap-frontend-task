{
  "extends": [
    "react-app",
    "react-app/jest",
    "airbnb",
    "prettier",
    "prettier/react",
    "plugin:jsx-a11y/recommended"
  ],
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "jest": true
  },
  "plugins": ["prettier", "react", "react-hooks"],
  "rules": {
    "react/jsx-props-no-spreading": "off"
  }
}
